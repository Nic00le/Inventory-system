{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  name: 'MaterialForm',\n  props: {\n    id: String,\n    newRecord: Boolean\n  },\n  setup(props) {\n    const records = ref({});\n    onMounted(async () => {\n      if (props.id) {\n        var response = await fetch(\"/api/records/\" + props.id);\n        if (response.ok) {\n          records.value = await response.json();\n        }\n      }\n    });\n    const submitRecords = async function () {\n      records.value.type = \"materials\";\n      var response = await fetch(\"/api/material/detail/\", {\n        method: \"post\",\n        headers: {\n          // 'Content-Type': 'application/x-www-form-urlencoded',\n          'Content-Type': 'application/json'\n        },\n        // body: new URLSearchParams(new FormData(event.target))\n        body: JSON.stringify(records.value)\n      });\n      console.log(records.value);\n      if (response.ok) {\n        var text = await response.text();\n        alert(text);\n        // location.assign(\"/bookings\")\n      } else {\n        alert(response.statusText);\n      }\n    };\n    const updateRecords = async function () {\n      delete records.value._id;\n      var response = await fetch(\"/api/record/update/\" + props.id, {\n        method: \"put\",\n        headers: {\n          // 'Content-Type': 'application/x-www-form-urlencoded',\n          'Content-Type': 'application/json'\n        },\n        // body: new URLSearchParams(new FormData(event.target))\n        body: JSON.stringify(records.value)\n      });\n      console.log(records.value);\n      if (response.ok) {\n        var text = await response.text();\n        alert(text);\n        // location.assign(\"/bookings\")\n      } else {\n        alert(response.statusText);\n      }\n    };\n    const deleteRecords = async function () {\n      console.log(props.id);\n      var response = await fetch(\"/api/record/delete/\" + props.id, {\n        method: \"delete\",\n        headers: {\n          // 'Content-Type': 'application/x-www-form-urlencoded',\n          'Content-Type': 'application/json'\n        }\n\n        // body: new URLSearchParams(new FormData(event.target))\n        // body: JSON.stringify(records.value)\n      });\n\n      if (response.ok) {\n        // var text = await response.text();\n        alert(\"Booking is deleted\");\n        // location.assign(\"/bookings\")\n      } else {\n        alert(response.statusText);\n      }\n    };\n    return {\n      submitRecords,\n      deleteRecords,\n      updateRecords,\n      records\n    };\n  }\n};","map":{"version":3,"mappings":"AAqDA,SAASA,GAAG,EAAEC,SAAS,QAAO,KAAK;AAEnC,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAC;IACFC,EAAE,EAAEC,MAAM;IACVC,SAAS,EAAEC;EACf,CAAC;EACDC,KAAK,CAACL,KAAK,EAAE;IAET,MAAMM,OAAM,GAAIT,GAAG,CAAC,CAAC,CAAC,CAAC;IAEvBC,SAAS,CAAC,YAAY;MAClB,IAAIE,KAAK,CAACC,EAAE,EAAE;QACV,IAAIM,QAAO,GAAI,MAAMC,KAAK,CAAC,eAAc,GAAIR,KAAK,CAACC,EAAE;QAErD,IAAIM,QAAQ,CAACE,EAAE,EAAE;UACbH,OAAO,CAACI,KAAI,GAAI,MAAMH,QAAQ,CAACI,IAAI,EAAE;QACzC;MACJ;IACJ,CAAC,CAAC;IAEF,MAAMC,aAAY,GAAI,kBAAkB;MACpCN,OAAO,CAACI,KAAK,CAACG,IAAG,GAAI,WAAU;MAE/B,IAAIN,QAAO,GAAI,MAAMC,KAAK,CAAC,uBAAuB,EAAE;QAChDM,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL;UACA,cAAc,EAAE;QACpB,CAAC;QAED;QACAC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACZ,OAAO,CAACI,KAAK;MACtC,CAAC,CAAC;MACFS,OAAO,CAACC,GAAG,CAACd,OAAO,CAACI,KAAK;MACzB,IAAIH,QAAQ,CAACE,EAAE,EAAE;QAEb,IAAIY,IAAG,GAAI,MAAMd,QAAQ,CAACc,IAAI,EAAE;QAChCC,KAAK,CAACD,IAAI,CAAC;QACX;MACJ,OAAO;QACHC,KAAK,CAACf,QAAQ,CAACgB,UAAU;MAC7B;IAEJ;IAEA,MAAMC,aAAY,GAAI,kBAAkB;MACpC,OAAOlB,OAAO,CAACI,KAAK,CAACe,GAAG;MAExB,IAAIlB,QAAO,GAAI,MAAMC,KAAK,CAAC,qBAAoB,GAAIR,KAAK,CAACC,EAAE,EAAE;QACzDa,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE;UACL;UACA,cAAc,EAAE;QACpB,CAAC;QAED;QACAC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACZ,OAAO,CAACI,KAAK;MACtC,CAAC,CAAC;MACFS,OAAO,CAACC,GAAG,CAACd,OAAO,CAACI,KAAK;MACzB,IAAIH,QAAQ,CAACE,EAAE,EAAE;QAEb,IAAIY,IAAG,GAAI,MAAMd,QAAQ,CAACc,IAAI,EAAE;QAChCC,KAAK,CAACD,IAAI,CAAC;QACX;MACJ,OAA